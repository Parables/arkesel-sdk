{
    "name": "laravel-notification-channels/arkesel",
    "description": "Arkesel Notification Channel For Laravel",
    "homepage": "https://github.com/laravel-notification-channels/arkesel",
    "license": "MIT",
    "authors": [
        {
            "name": "Parables Boltnoel",
            "email": "parables95@gmail.com",
            "homepage": "https://github.com/parables",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "^7.4",
        "illuminate/notifications": "~6.0 || ~7.0 || ~8.0",
        "illuminate/support": "~6.0 || ~7.0 || ~8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\Arkesel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\Arkesel\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    }
}
