{
    "name": "parables/laravel-arkesel-sms-notification",
    "description": "Arkesel Notification Channel For Laravel",
    "homepage": "https://github.com/parables/laravel-arkesel-sms-notification",
    "license": "MIT",
    "authors": [
        {
            "name": "Parables Boltnoel",
            "email": "parables95@gmail.com",
            "homepage": "https://github.com/parables",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Parables\\ArkeselSmsNotification\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Parables\\ArkeselSmsNotification\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-text --coverage-clover=coverage.clover"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Parables\\ArkeselSmsNotification\\ArkeselServiceProvider"
            ]
        }
    }
}
